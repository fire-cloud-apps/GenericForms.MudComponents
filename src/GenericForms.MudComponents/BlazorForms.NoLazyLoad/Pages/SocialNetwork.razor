    <MudText Class="ml-4" Typo="Typo.h5">Social Network Component</MudText>

    <SocialNetworkLogin FacebookLink="https://facebook.fc/login" GoogleLink="Google" MicrosoftLink="Microsoft" TwitterLink="Twitter" />

</MudPaper>


