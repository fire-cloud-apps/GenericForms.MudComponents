@page "/authentication/social-network"

<MudBreadcrumbs Items="_items"></MudBreadcrumbs>

@code {
    private List<BreadcrumbItem> _items = new List<BreadcrumbItem>
    {
        new BreadcrumbItem("Authentication", href: "/", 
            icon: Icons.Material.Outlined.Security),

        new BreadcrumbItem("Social Network", 
            icon:Icons.Material.Outlined.SocialDistance, disabled:false,
            href:  "/authentication/social-network"),
    };
}

